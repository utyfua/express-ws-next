{
  "name": "express-ws-next",
  "version": "6.0.0",
  "description": "WebSocket endpoints for Express applications",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js",
      "types": "./esm/index.d.ts"
    },
    "./*": {
      "import": "./esm/*.js",
      "require": "./cjs/*.js",
      "types": "./esm/*.d.ts"
    },
    "./src/*": "./src/*"
  },
  "scripts": {
    "build": "sh build.sh"
  },
  "author": "Utyfua <utyfua@gmail.com>",
  "contributors": [
    "Jesús Leganés Combarro <piranna@gmail.com>",
    "Sven Slootweg <admin@cryto.net>",
    "Andrew Phillips <theasp@gmail.com>",
    "Nicholas Schell <nschell@gmail.com>",
    "Max Truxa <dev@maxtruxa.com>",
    "Kræn Hansen <mail@kraenhansen.dk>",
    "Alexis Tyler <xo@wvvw.me>",
    "Utyfua <utyfua@gmail.com>"
  ],
  "license": "BSD-2-Clause",
  "dependencies": {
    "ws": "^8.18.1"
  },
  "peerDependencies": {
    "express": "^4.0.0 || ^5.0.0"
  },
  "engines": {
    "node": ">=4.5.0"
  },
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/utyfua/express-ws-next"
  },
  "keywords": [
    "express",
    "ws",
    "websocket"
  ],
  "bugs": {
    "url": "https://github.com/utyfua/express-ws-next/issues"
  },
  "homepage": "https://github.com/utyfua/express-ws-next",
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "^22.13.5",
    "@types/ws": "^8.5.14",
    "copyfiles": "^2.4.1",
    "express": "4.21.2",
    "typescript": "^5.7.3"
  }
}
